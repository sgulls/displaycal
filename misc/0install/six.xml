<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://zero-install.sourceforge.net/2004/injector/interface http://0install.de/schema/injector/interface/interface.xsd http://0install.de/schema/desktop-integration/capabilities http://0install.de/schema/desktop-integration/capabilities/capabilities.xsd" uri="${URL}0install/six.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
	<publish mode="package" xmlns="${URL}" />
	<name>six</name>
	<summary xml:lang="en">Python 2 and 3 compatibility utilities</summary>
	<description xml:lang="en">Six is a Python 2 and 3 compatibility library. It provides utility functions for smoothing over the differences between the Python versions with the goal of writing Python code that is compatible on both Python versions. See the documentation for more information on what is provided.</description>
	<homepage>http://pypi.python.org/pypi/six/</homepage>
	<package-implementation package="python-six" distributions="RPM" />
	<package-implementation package="python-six" distributions="Debian" />
	<package-implementation package="dev-python/six" distributions="Gentoo" />
	<package-implementation package="python2-six" distributions="Arch" />
	<group license="MIT License">
		<requires interface="${URL}0install/python.xml" version="2.6..!4.0">
			<version not-before="2.6" before="4.0" />
		</requires>
		<implementation version="0.0.0" released="0000-00-00" stability="stable">
			<environment name="PYTHONPATH" insert="." />
		</implementation>
	</group>
</interface>
